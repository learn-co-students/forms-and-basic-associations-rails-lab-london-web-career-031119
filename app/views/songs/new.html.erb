<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :genre %>
 <%= f.collection_select :genre_id, @genres, :id, :name %>

 <input name='song[notes][]' id='song_notes_1'>
<input name='song[notes][]' id='song_notes_2'>
<input name='song[notes][]' id='song_notes_3'>

  <%= f.submit %>
<% end %>


