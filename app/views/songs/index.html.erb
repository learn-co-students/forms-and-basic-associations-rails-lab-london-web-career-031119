<h1>All songs</h1>

<% @songs.each do |song| %>
  <li><span class=song-title><%= link_to song.title, song %> by </span>
  <span class=song-artist><%= link_to song.try(:artist).try(:name), song.artist %> </span>
  <span class=song-genre>(<%= song.try(:genre).try(:name) %>) </span>
  <ul class=song-notes>
    <% song.try(:notes).try :each do |note| %>
      <li class=song-note><%= note.content %></li>
    <% end %>
  </ul></li>
<% end %>
